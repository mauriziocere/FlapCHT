FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvOptions;
}

heatFluxSource
{
    type            externalWallHeatFluxTemperatureSource;
    active          true;
    selectionMode   all; // oppure region o faces se vuoi filtrare

    externalWallHeatFluxTemperatureSourceCoeffs
    {
        patches         ("flapHole" "defaultFaces");
        heatSourceType  externalFlux;
        q               preciceMapped; // questo dice che arriva da preCICE
        preciceParticipant  Fluid;
        preciceMesh         Fluid-Mesh;
        preciceData         heat-flux;
    }
}

